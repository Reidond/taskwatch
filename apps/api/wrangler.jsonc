{
	"name": "taskwatch-api",
	"main": "src/index.ts",
	"compatibility_date": "2024-12-30",
	"compatibility_flags": ["nodejs_compat"],
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "taskwatch-db",
			"database_id": "4c03274a-0856-491b-92de-f55748e7725b"
		}
	],
	"triggers": {
		"crons": ["*/5 * * * *"]
	},
	"vars": {
		"ENVIRONMENT": "production"
	},
	"routes": [
		{
			"pattern": "taskwatch.andriishafar.xyz/api/*",
			"zone_name": "andriishafar.xyz"
		}
	]
}
